<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>[RF] Fertile Fields [1.0]</li>
        </mods>
        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
            
                <li Class="TinyTweaks.PatchOperationCheckModSetting">
                    <settingsType>TinyTweaks.TinyTweaksSettings</settingsType>
                    <settingName>compostBillsExcludeRaw</settingName>
                </li>

                <li Class="PatchOperationRemove">
                    <xpath>/Defs/RecipeDef[defName="MakeCompost" or defName="MakeCompost5"]/defaultIngredientFilter/categories</xpath>
                </li>

            </operations>
        </match>
    </Operation>

</Patch>